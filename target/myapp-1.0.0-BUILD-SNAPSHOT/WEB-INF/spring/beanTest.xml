<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">


<bean id="loginVO" class="com.kh.myapp.login.vo.LoginVO">
	<property name="id" value="conbean@test.com"></property>
	<property name="passwd" value="conbean"></property>
</bean> 


<bean id="conLoginVO" class="com.kh.myapp.login.vo.LoginVO">
	<constructor-arg ref="loginVO" />
</bean>

</beans>
